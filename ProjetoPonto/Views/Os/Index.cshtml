@model IEnumerable<ProjetoPonto.Entity.Os>

@{
    ViewBag.Title = "Index";
}
<body>
  <a  name="services"></a>
    <div class="content-section-a">

        <div class="container">
            <div class="row">
                 <p>
     @Html.ActionLink("Novo", "Edit", new { id = 0 }, new { @class = "btn btn-primary", @style = "color:white" }) 
</p>
                  

                    <div class="clearfix"></div>

    
        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="panel-heading">
                    Gerenciamento de OS
                </div>
                <!-- /.panel-heading -->
                <div class="panel-body">
                    <div class="table-responsive">
                        <table class="table table-striped table-bordered table-hover">
                            <thead>
                                <tr>
        <th>
            @Html.DisplayNameFor(model => model.NumeroOs)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Observacao)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Serie)
        </th>
        <th>
           
            @Html.DisplayNameFor(model => model.DataAbertura);
        </th>

        <th>
            @Html.DisplayNameFor(model => model.DataFechamento)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cliente.Nome)
        </th>
         <th>
            @Html.DisplayNameFor(model => model.StatusOs.Descricao)
        </th>
         <th>
            @Html.DisplayNameFor(model => model.ModeloMaquina.Descricao)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.IdOs)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NumeroOs)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Observacao)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Serie)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DataAbertura)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DataFechamento)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Cliente.Nome)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StatusOs.Descricao)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ModeloMaquina.Descricao)
        </td>
                <td>
            @Html.DisplayFor(modelItem => item.IdOs)
        </td>
   <td>
            @Html.ActionLink("Editar", "Edit", new { id = item.IdOs }, new { @class = "btn btn-primary", @style = "color:white" }) 
            @Html.ActionLink("Delete", "Delete", new { id = item.IdOs }, new { @class = "btn btn-primary", @style = "color:white", @onclick = "return confirm('Tem certeza?');" })
        </td>
    </tr>
}


                        </table>

                    </div>
                    <!-- /.table-responsive -->
                </div>
                <!-- /.panel-body -->
            </div>
            <!-- /.panel -->
        </div>
        </div>  
            </div>
        </div>
   
   
    
</body>